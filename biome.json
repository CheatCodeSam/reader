{
	"$schema": "https://biomejs.dev/schemas/1.7.3/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
	"javascript": {
		"formatter": {
			"semicolons": "asNeeded",
			"indentWidth": 4,
			"indentStyle": "space"
		}
	},
	"json": {
		"formatter": {
			"indentWidth": 4,
			"indentStyle": "space"
		}
	}
}